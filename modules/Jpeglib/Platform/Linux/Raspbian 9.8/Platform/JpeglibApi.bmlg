namespace Platform.JpegLib {
	public static partial struct JpegLibApi {
		public static void jpeg_create_compress( jpeg_compress_struct* info ) { jpeg_CreateCompress( info, Version, sizeof( *info ) ); }
		public static void jpeg_create_decompress( jpeg_decompress_struct* info ) { jpeg_CreateDecompress( info, Version, sizeof( *info ) ); }
	}
}