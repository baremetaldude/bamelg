		public static Convert.ConversionResult TryConvert( thistype* result, TimeSpan value ) {
			if( value.Ticks >= 21474836479999990 ) { *result = MaxValue; return Convert.ConversionResult.Clamped; }
			if( value.Ticks <= -21474836480000000 ) { *result = MinValue; return Convert.ConversionResult.Clamped; }

			result->tv_sec = ( int ) value.TotalSeconds;
			result->tv_nsec = ( int )( value.Ticks % TimeSpan.TicksPerSecond * 100 );

			if( result->tv_nsec < 0 ) {
				result->tv_nsec += 1000000000;
				if( checkwrap( --result->tv_sec ) ) { *result = MinValue; return Convert.ConversionResult.Clamped; }
			}

			return Convert.ConversionResult.OK;
		}

		public static Convert.ConversionResult TryConvert( thistype* result, TimeDuration value ) {
			if( value.Ticks >= 21474836479999990 ) { *result = MaxValue; return Convert.ConversionResult.Clamped; }

			result->tv_sec = ( int ) value.TotalSeconds;
			result->tv_nsec = ( int )( value.Ticks % TimeDuration.TicksPerSecond * 100U );
			result->EnsureNormalized();

			return Convert.ConversionResult.OK;
		}

