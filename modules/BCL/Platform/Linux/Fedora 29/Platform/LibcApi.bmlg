	public partial struct tm : IFormattable {
		public static Convert.ConversionResult TryConvert( DateTimeUTC* result, thistype value ) {
			// if( value.Day > 31 || ( value.Month - 1 ) > 11 ) { *result = DateTimeUTC.MaxValue; return Convert.ConversionResult.IncompatibleValue; }
			// if( value.Day > 31 || ( value.Month - 1 ) > 11 ) { *result = DateTimeUTC.MaxValue; return Convert.ConversionResult.IncompatibleValue; }

			ulong dateTicks;
			if( !DateValues.TryDateToTicks( dateTicks, 1900 + ( ushort ) value.tm_year, ( byte ) value.tm_mon + 1, ( byte ) value.tm_mday ) ) { *result = DateTimeUTC.MaxValue; return Convert.ConversionResult.IncompatibleValue; }

			ulong timeTicks;
			if( !DateValues.TryTimeToTicks( timeTicks, ( byte ) value.tm_hour, ( byte ) value.tm_min, ( byte ) value.tm_sec ) ) { *result = DateTimeUTC.MaxValue; return Convert.ConversionResult.IncompatibleValue; }

			if( checkwrap( result->Ticks = dateTicks + timeTicks ) ) { *result = DateTimeUTC.MaxValue; return Convert.ConversionResult.Clamped; }

			return Convert.ConversionResult.OK;
		}

		public static Convert.ConversionResult TryConvert( thistype* result, DateTimeUTC value ) {
			var dateValues = value.GetDateValues();

			result->tm_year = dateValues.Year - 1900;
			result->tm_mon = dateValues.Month - 1;
			result->tm_wday = ( dateValues.DayOfWeek.Value() + 1 ) % 7;
			result->tm_mday = dateValues.Day;
			result->tm_hour = value.Hour;
			result->tm_min = value.Minute;
			result->tm_sec = value.Second;
			result->tm_isdst = 0;
			result->tm_yday = dateValues.DayOfYear - 1;
			result->tm_gmtoff = 0;
			result->tm_zone = ( sbyte* ) cast<SbcsString>( "GMT" ).GetChars();

			return Convert.ConversionResult.OK;
		}

		public static explicit operator thistype( DateTimeUTC value ) { thistype result; Assert.IsTrue( TryConvert( &result, value ) ); return result; }
		public static explicit operator DateTimeUTC( thistype value ) { DateTimeUTC result; Assert.IsTrue( TryConvert( &result, value ) ); return result; }

		public void ToString( StringBuilder builder, string format ) {
			if( format == null ) {
				builder.Append( 1900 + tm_year, "D4" );
				builder.Append( '-' );
				builder.Append( tm_mon + 1, "D2" );
				builder.Append( '-' );
				builder.Append( tm_mday, "D2" );
				builder.Append( ' ' );
				builder.Append( tm_hour, "D2" );
				builder.Append( ':' );
				builder.Append( tm_min, "D2" );
				builder.Append( ':' );
				builder.Append( tm_sec, "D2" );

				builder.Append( " tm_isdst:" );
				builder.Append( tm_isdst );

				builder.Append( " tm_wday:" );
				builder.Append( tm_wday );

				builder.Append( " tm_yday:" );
				builder.Append( tm_yday );

				builder.Append( " tm_gmtoff:" );
				builder.Append( tm_gmtoff );

				builder.Append( " tm_zone:" );
				builder.Append( new CString( tm_zone ) );
			}
			else
				builder.Append( ( DateTimeUTC ) this, "Q" );
		}

		public static bool operator ==( thistype left, thistype right ) {
			return left.tm_year == right.tm_year && left.tm_mon == right.tm_mon && left.tm_mday == right.tm_mday && left.tm_hour == right.tm_hour && left.tm_min == right.tm_min && left.tm_sec == right.tm_sec && left.tm_isdst == right.tm_isdst && left.tm_wday == right.tm_wday && left.tm_yday == right.tm_yday && left.tm_gmtoff == right.tm_gmtoff && new CString( left.tm_zone ) == new CString( right.tm_zone );
		}

		public static bool operator !=( thistype left, thistype right ) { return !( left == right ); }
	}

