			if( ticks <= 599881131520000000 ) { *result = MinValue; return Convert.ConversionResult.Clamped; }

			result->tv_sec = ( int )( ticks / TimeSpan.TicksPerSecond - TimeSpan.Ticks_1970_1_1 / TimeSpan.TicksPerSecond );
			result->tv_nsec = ( int )( ticks % TimeSpan.TicksPerSecond * 100 - TimeSpan.Ticks_1970_1_1 % TimeSpan.TicksPerSecond * 100 );
			result->EnsureNormalized();

			return Convert.ConversionResult.OK;
		}

