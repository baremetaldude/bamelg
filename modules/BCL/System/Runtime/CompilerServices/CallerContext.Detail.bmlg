namespace System.Runtime.CompilerServices {
	public partial struct CallerContext : IFormattable {
		void IFormattable.ToString( StringBuilder builder, string format ) {
			builder.Concat( Path, ':', Row, '(', Caller, " -> ", Expression, ')' );
		}
	}
}