namespace System {
	public struct RgbColor : IFormattable {
		public byte R, G, B;

		public RgbColor() { }

		public RgbColor( byte r, byte g, byte b ) {
			R = r;
			G = g;
			B = b;
		}

		public void ToString( StringBuilder builder, string format ) {
			switch( format ) {
				case "w":
					builder.Append( '#' );
					builder.Append( R, "X2" );
					builder.Append( G, "X2" );
					builder.Append( B, "X2" );
					break;

				default:
					builder.AppendFormat( "{{ R={0} G={1} B={2} }}", R, G, B );
					break;
			}
		}

		public static explicit operator RgbColor( RgbFloatingColor value ) { return new RgbColor { R = ( byte )( value.R * 255 ), G = ( byte )( value.G * 255 ), B = ( byte )( value.B * 255 ) }; }
		public static explicit operator RgbColor( RgbaColor value ) { return new RgbColor { R = value.R, G = value.G, B = value.B }; }

		public static explicit operator RgbColor( ColorValue.Rgb value ) { return new RgbColor { R = ( byte )( value.R * 255 ), G = ( byte )( value.G * 255 ), B = ( byte )( value.B * 255 ) }; }

