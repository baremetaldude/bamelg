	public struct RgbaColor {
		public byte R, G, B, A;

		public RgbaColor() { }

		public RgbaColor( byte r, byte g, byte b ) {
			R = r;
			G = g;
			B = b;
			A = 255;
		}

		public RgbaColor( byte r, byte g, byte b, byte a ) {
			R = r;
			G = g;
			B = b;
			A = a;
		}

		public RgbaColor( RgbColor @base, byte a ) {
			R = @base.R;
			G = @base.G;
			B = @base.B;
			A = a;
		}

		public static explicit operator RgbaColor( RgbaFloatingColor value ) { return new RgbaColor { A = ( byte )( value.A * 255 ), R = ( byte )( value.R * 255 ), G = ( byte )( value.G * 255 ), B = ( byte )( value.B * 255 ) }; }

		public static explicit operator RgbaColor( ColorValue.Rgb value ) { return new RgbaColor { R = ( byte )( value.R * 255 ), G = ( byte )( value.G * 255 ), B = ( byte )( value.B * 255.0 ), A = 255 }; }

