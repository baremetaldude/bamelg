namespace System.Hash {
	public static struct Checksum {
		public static uintptr Compute8( void* data, uint length ) {
			uintptr result = 0;
			var ptr = ( byte* ) data;

			while( --length != uint.MaxValue )
				result += *ptr++;

			return result;
		}
	}
}