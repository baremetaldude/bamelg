		public static struct Fixed {
			public struct KiloFormatter32 : IFormattable {
				public uint Value;

				public void ToString( StringBuilder builder, string format ) {
					if( Value % 1000U == 0 ) {
						builder.Append( Value / 1000U );
						AddPrefix_K( builder );
					}
					else if( Value % 100U == 0U ) {
						builder.Append( Value / 100U, "Q1" );
						AddPrefix_K( builder );
					}
					else if( Value % 10 == 0 ) {
						builder.Append( Value / 10U, "Q2" );
						AddPrefix_K( builder );
					}
					else {
						builder.Append( Value, "Q3" );
						AddPrefix_K( builder );
					}
				}
			}

