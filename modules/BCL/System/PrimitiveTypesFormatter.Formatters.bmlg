		public static struct Suffix {
			public struct Byte : IFormattable {
				public void ToString( StringBuilder builder, string format ) {
					switch( CultureInfo.Current.Kind ) {
						case CultureInfo.CultureKind.RU: builder.Append( 'Б' ); break;

						case CultureInfo.CultureKind.EN:
						default: builder.Append( 'B' ); break;
