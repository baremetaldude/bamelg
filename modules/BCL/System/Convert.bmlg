			// if( value == DateTimeUTC.MaxValue ) {
			// 	*result = DateTimeLocal.MaxValue;
			// 	return Convert.ConversionResult.OK;
			// }

			var offset = TimeZone.GetLocalFromUtcOffset( value );
			if( new DateTimeLocal( value.Ticks ).TryAddTicks( *result, offset ) )
				return Convert.ConversionResult.OK;

			if( offset >= 0 ) *result = DateTimeLocal.MaxValue;
			else *result = DateTimeLocal.MinValue;
			return Convert.ConversionResult.Clamped;
		}

