		public static ConversionResult TryConvert( DateTimeUTC* result, DateTime value ) {
			if( value.Ticks >= DateTime.MaxTicks ) {
				*result = DateTimeUTC.MaxValue;
				return Convert.ConversionResult.OK;
			}

			if( value.Ticks == 0 ) {
				*result = DateTimeUTC.MinValue;
				return Convert.ConversionResult.OK;
			}

			result->Ticks = ( ulong ) value.ToUniversalTime().Ticks;
			return Convert.ConversionResult.OK;
		}

		public static ConversionResult TryConvert( DateTimeUTC* result, DateTimeLocal value ) {
			// if( value == DateTimeLocal.MaxValue ) {
			// 	*result = DateTimeUTC.MaxValue;
			// 	return Convert.ConversionResult.OK;
			// }

			// if( value == DateTimeLocal.MinValue ) {
			// 	*result = DateTimeUTC.MinValue;
			// 	return Convert.ConversionResult.OK;
			// }

			long utcOffset = TimeZone.GetUtcFromLocalOffset( value );
			if( new DateTimeUTC( value.Ticks ).TrySubtractTicks( *result, utcOffset ) ) {
				long localOffset = TimeZone.GetLocalFromUtcOffset( result->Ticks );
				if( utcOffset == localOffset )
					return Convert.ConversionResult.OK;

				return Convert.ConversionResult.Inexact;
			}

			if( utcOffset >= 0 ) *result = DateTimeUTC.MinValue;
			else *result = DateTimeUTC.MaxValue;
			return Convert.ConversionResult.Clamped;
		}

		public static ConversionResult TryConvert_AllowInexact( DateTimeUTC* result, DateTimeLocal value ) {
